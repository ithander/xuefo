<view><view class="text-box" scroll-y="true"><text style="align-content:center;">{{text}}</text></view><view class="btns"><button type="primary" size="mini" data-event-opts="{{[['tap',[['addValue',[1000]]]]]}}" bindtap="__e">加1000</button><button type="primary" size="mini" data-event-opts="{{[['tap',[['addValue',[100]]]]]}}" bindtap="__e">加100</button><button type="primary" size="mini" data-event-opts="{{[['tap',[['addValue',[10]]]]]}}" bindtap="__e">加10</button><button type="primary" size="mini" data-event-opts="{{[['tap',[['addValue',[1]]]]]}}" bindtap="__e">加1</button></view><view><button type="default" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e">返回</button></view><view><uni-list vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{months}}" wx:for-item="item" wx:for-index="index" wx:key="index"><scroll-view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['toDetail',['$0','$1'],[[['months','',index,'work']],[['months','',index,'date']]]]]]]}}" bindtap="__e"><uni-list-item vue-id="{{('2-'+index)+','+('1')}}" show-badge="{{true}}" note="{{item.date}}" show-arrow="{{false}}" title="{{item.work}}" badge-text="{{item.value}}" bind:__l="__l"></uni-list-item></scroll-view></block></uni-list></view></view>