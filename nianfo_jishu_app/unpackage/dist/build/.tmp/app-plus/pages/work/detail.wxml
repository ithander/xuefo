<view><view class="text-box" scroll-y="true"><text style="align-content:center;">{{text}}</text></view><view><button type="default" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e">返回</button></view><view><uni-list vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{months}}" wx:for-item="item" wx:for-index="index" wx:key="index"><scroll-view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['toDetail',['$0','$1'],[[['months','',index,'work']],[['months','',index,'date']]]]]]]}}" bindtap="__e"><uni-list-item vue-id="{{('2-'+index)+','+('1')}}" show-badge="{{true}}" note="{{item.date}}" show-arrow="{{false}}" title="{{item.work}}" badge-text="{{item.value}}" bind:__l="__l"></uni-list-item></scroll-view></block></uni-list></view><view><uni-fab vue-id="3" pattern="{{pattern}}" horizontal="{{horizontal}}" vertical="{{vertical}}" content="{{content}}" direction="{{direction}}" data-event-opts="{{[['^trigger',[['trigger']]]]}}" bind:trigger="__e" bind:__l="__l"></uni-fab></view></view>